<h1>Followers</h1>

  <% @users.each do |user| %>
    <hr>
    <p><span>email: </span><%= link_to user.email, user_path(user.id) %></p>
      <% if current_user != user %>
        <% if user.followed_by?(current_user) %>
          <p><%= link_to "Followæ¸ˆ", user_relationships_path(user.id), method: :delete %></p>
        <% else %>
          <p><%= link_to "Follow?", user_relationships_path(user.id), method: :post %></p>
        <% end %>
      <% end %>
  <% end %>